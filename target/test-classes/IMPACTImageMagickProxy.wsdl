                    <xsd:element maxOccurs="1" minOccurs="0" name="outputUrl" type="xsd:anyURI"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:element name="ByUrlResponse" type="tns:ByUrlResponseType"/>

